{
    "timeline_scaling_factor": 1.0,
    "grouping_window": 5.0,
    "sound_files_folder": "sounds",
    "mapping_rules": {
        "users": [
            {
                "pattern": ".*dev.*",
                "sound_file": "developer.wav"
            },
            {
                "pattern": ".*admin.*",
                "sound_file": "admin.wav"
            }
        ],
        "actions": [
            {
                "pattern": "A",
                "sound_file": "swoosh.wav"
            },
            {
                "pattern": "M",
                "sound_file": "explosion3.wav"
            },
            {
                "pattern": "D",
                "sound_file": "explosion1.wav"
            }
        ]
    }
}